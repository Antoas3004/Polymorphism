public class OverLoading {

	void add(int a, int b) {
		int c = a + b;
		System.out.println(c);
	}

	void add(int a, int b, int c) {
		int d = a + b + c;
		System.out.println(d);
	}

	public static void main(String[] args) {
		OverLoading obj = new OverLoading();
		obj.add(5, 5);
		obj.add(5, 5, 5);
	}
}
